{
  "iconsDir": "public",
  "source": ".\/assets\/media\/icon.svg",
  "manifest": {
    "appName": "Club de Voile de Sevrier",
    "appShortName": "CVSevrier",
    "appDescription": "Site du Club de Voile de Sevrier",
    "background": "#fff",
    "theme_color": "#1e515d",
    "lang": "fr-FR",
    "preferRelatedApplications": false,
    "pixel_art": false,
    "loadManifestWithCredentials": false,
    "manifestMaskable": false,
    "icons": {
      "favicons": true,
      "android": true,
      "appleIcon": true,
      "appleStartup": false,
      "coast": false,
      "yandex": false,
      "windows": false
    }
  },
  "disable": {
    "rss": true,
    "comment": true,
    "emoji": true,
    "media": true,
    "oembed": true,
    "xmlrpc": true,
    "rest_user": true,
    "jquery": true
  }
}
