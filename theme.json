{
  "$schema": "https://schemas.wp.org/wp/6.2/theme.json",
  "customTemplates": [],
  "settings": {
    "useRootPaddingAwareAlignments": true,
    "appearanceTools": true,
    "color": {
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "duotone": [],
      "link": true,
      "text": true,
      "background": true,
      "defaultGradients": false,
      "defaultPalette": false,
      "gradients": [],
      "palette": [
        {
          "color": "#000000",
          "name": "Black",
          "slug": "black"
        },
        {
          "color": "#4a4a49",
          "name": "Dark Gray",
          "slug": "dark-gray"
        },
        {
          "color": "#9E9E9D",
          "name": "Light Gray",
          "slug": "light-gray"
        },
        {
          "color": "#ffffff",
          "name": "White",
          "slug": "white"
        },
        {
          "color": "#00A7BF",
          "name": "Blue Logo",
          "slug": "blue-logo"
        },
        {
          "color": "#2f7e92",
          "name": "Light Blue",
          "slug": "light-blue"
        },
        {
          "color": "#1e515d",
          "name": "Dark Blue",
          "slug": "dark-blue"
        },
        {
          "color": "#2E2E2D",
          "name": "Gray",
          "slug": "text-gray"
        },
        {
          "color": "#E20C41",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#FEF4F4",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#D59A16",
          "name": "Tertiary",
          "slug": "tertiary"
        },
        {
          "color": "#fff1e1",
          "name": "Quaternary",
          "slug": "quaternary"
        }
      ]
    },
    "custom": {
      "typography": {
        "line-height": {
          "x-small": 1,
          "small": 1.1,
          "medium": 1.2,
          "large": 1.5,
          "x-large": 1.7,
          "2x-large": 1.9,
          "3x-large": 2,
          "4x-large": 2.3
        }
      }
    },
    "spacing": {
      "blockGap": true,
      "padding": true,
      "margin": true,
      "spacingSizes": [
        {
          "size": "clamp(0.5rem, 0.8vw, 1.1rem)",
          "slug": "10",
          "name": "10"
        },
        {
          "size": "clamp(0.5rem, 1vw, 1.5rem)",
          "slug": "20",
          "name": "20"
        },
        {
          "size": "clamp(1.5rem, 5vw, 2rem)",
          "slug": "30",
          "name": "30"
        },
        {
          "size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
          "slug": "40",
          "name": "40"
        },
        {
          "size": "clamp(2.2rem, 7vw, 6rem)",
          "slug": "50",
          "name": "50"
        },
        {
          "size": "clamp(2.6rem, 9vw, 6.5rem)",
          "slug": "60",
          "name": "60"
        }
      ],
      "units": [
        "%",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "fluid": true,
      "textTransform": true,
      "fontStyle": true,
      "customFontSize": true,
      "textDecoration": true,
      "lineHeight": true,
      "letterSpacing": true,
      "fontWeight": true,
      "dropCap": true,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Sora",
              "fontStretch": "25% 151%",
              "fontWeight": "100 800",
              "fontStyle": "oblique 0deg 10deg",
              "unicodeRange": "U+000-5FF;",
              "src": [
                "file:./assets/fonts/Sora-VariableFont_wght.woff2"
              ]
            }
          ],
          "fontFamily": "\"Sora\", Arial, sans-serif",
          "name": "Sora",
          "slug": "Sora"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Montserrat",
              "fontStretch": "25% 151%",
              "fontWeight": "100 800",
              "fontStyle": "oblique 0deg 10deg",
              "unicodeRange": "U+000-5FF;",
              "src": [
                "file:./assets/fonts/Montserrat-VariableFont_,wght-latin.woff2"
              ]
            }
          ],
          "fontFamily": "\"Montserrat\", Arial, sans-serif",
          "name": "Montserrat",
          "slug": "montserrat"
        }
      ],
      "fontSizes": [
        {
          "fluid": false,
          "size": "0.75rem",
          "slug": "x-small"
        },
        {
          "fluid": false,
          "size": "0.875rem",
          "slug": "small"
        },
        {
          "fluid": false,
          "size": "1rem",
          "slug": "medium"
        },
        {
          "fluid": false,
          "size": "1.125rem",
          "slug": "large"
        },
        {
          "size": "1.5rem",
          "slug": "x-large",
          "fluid": {
            "min": "1.25rem",
            "max": "1.5rem"
          }
        },
        {
          "size": "2rem",
          "slug": "2x-large",
          "fluid": {
            "min": "1.625rem",
            "max": "2rem"
          }
        },
        {
          "size": "3.125rem",
          "slug": "3x-large",
          "fluid": {
            "min": "2.5rem",
            "max": "3.125rem"
          }
        },
        {
          "size": "3.75rem",
          "slug": "4x-large",
          "fluid": {
            "min": "3.125rem",
            "max": "3.75rem"
          }
        },
        {
          "size": "4.125rem",
          "slug": "5x-large",
          "fluid": {
            "min": "3.875rem",
            "max": "4.125rem"
          }
        }
      ]
    },
    "layout": {
      "contentSize": "1090px",
      "wideSize": "1275px"
    }
  },
  "styles": {
    "spacing": {
      "blockGap": "1rem",
      "padding": {
        "top": "0",
        "right": "1rem",
        "bottom": "0",
        "left": "1rem"
      }
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": "25px"
        },
        "color": {
          "background": "var(--wp--preset--color--tertiary)",
          "text": "var(--wp--preset--color--white)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "core/post-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--medium)",
          "fontSize": "var(--wp--preset--font-size--3-x-large)"
        },
        "color": {
          "text": "var(--wp--preset--color--dark-blue)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--20)",
            "bottom": "var(--wp--preset--spacing--30)"
          }
        }
      },
      "core/pullquote": {
        "border": {
          "width": "1px 0"
        }
      },
      "core/query-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--2-x-large)",
          "fontSize": "var(--wp--preset--font-size--3-x-large)"
        }
      },
      "core/query": {
        "elements": {
          "h2": {
            "typography": {
              "fontSize": "var(--wp--preset--font-size--x-large)"
            },
            "spacing": {
              "padding": {
                "top": "var(--wp--preset--spacing--10)",
                "bottom": "var(--wp--preset--spacing--10)"
              }
            }
          }
        }
      },
      "core/quote": {
        "border": {
          "width": "1px"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--x-large)",
          "fontSize": "var(--wp--preset--font-size--2-x-large)"
        }
      },
      "core/paragraph": {
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--20)",
            "bottom": "var(--wp--preset--spacing--20)"
          }
        }
      },
      "core/gallery": {
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--20)",
            "bottom": "var(--wp--preset--spacing--20)"
          }
        }
      },
      "core/navigation": {
        "spacing": {
          "blockGap": ".2rem"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--white)",
      "text": "var(--wp--preset--color--text-gray)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--typography--line-height--medium)",
          "fontSize": "var(--wp--preset--font-size--4-x-large)"
        },
        "color": {
          "text": "var(--wp--preset--color--dark-blue)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--40)",
            "bottom": "var(--wp--preset--spacing--30)"
          }
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--typography--line-height--medium)",
          "fontSize": "var(--wp--preset--font-size--2-x-large)"
        },
        "color": {
          "text": "var(--wp--preset--color--dark-gray)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--30)",
            "bottom": "var(--wp--preset--spacing--30)"
          }
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--medium)",
          "fontSize": "var(--wp--preset--font-size--x-large)"
        },
        "color": {
          "text": "var(--wp--preset--color--dark-gray)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--20)",
            "bottom": "var(--wp--preset--spacing--30)"
          }
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--medium)",
          "fontSize": "var(--wp--preset--font-size--2-x-large)"
        },
        "color": {
          "text": "var(--wp--preset--color--dark-gray)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--medium)",
          "fontSize": "var(--wp--preset--font-size--2-x-large)"
        },
        "color": {
          "text": "var(--wp--preset--color--dark-gray)"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--roboto)",
          "fontWeight": "600",
          "lineHeight": "var(--wp--custom--typography--line-height--medium)",
          "fontSize": "var(--wp--preset--font-size--2-x-large)"
        },
        "color": {
          "text": "var(--wp--preset--color--dark-gray)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--light-blue)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--dark-blue)"
          }
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--montserrat)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "var(--wp--custom--typography--line-height--large)"
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ],
  "version": 2
}